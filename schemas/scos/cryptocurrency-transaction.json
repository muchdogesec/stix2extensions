{
	"$id": "https://raw.githubusercontent.com/muchdogesec/stix2extensions/main/schemas/scos/cryptocurrency-transaction.json",
	"$schema": "https://json-schema.org/draft/2020-12/schema",
	"title": "cryptocurrency-transaction",
	"description": "This extension creates a new SCO that can be used to represent cryptocurrency transactions.",
	"type": "object",
	"allOf": [
		{
			"$ref": "https://raw.githubusercontent.com/oasis-open/cti-stix2-json-schemas/master/schemas/common/cyber-observable-core.json"
		},
		{
			"properties": {
				"type": {
					"type": "string",
					"description": "The value of this property MUST be `cryptocurrency-transaction`.",
					"enum": ["cryptocurrency-transaction"]
				},
				"id": {
					"type": "string",
					"pattern": "^cryptocurrency-transaction--",
					"examples": ["cryptocurrency-transaction--da6911a9-7f49-5d20-ae4e-f9154d1b9a39"]
				},
				"symbol": {
					"type": "string",
					"description": "3 digit code for the cryptocurrency.",
					"examples": ["BTC"]
				},
				"value": {
					"type": "string",
					"description": "The transaction hash.",
					"examples": ["3FZbgi29cpjq2GjdwV8eyHuJJnkLtktZc5"]
				},
				"fee": {
					"type": "number",
					"description": "The transaction fee.",
					"examples": [0.00000728]
				},
				"execution_time": {
					"$ref": "https://raw.githubusercontent.com/oasis-open/cti-stix2-json-schemas/master/schemas/common/timestamp.json",
					"description": "The time of transaction.",
					"examples": ["2022-10-02T15:22:21.00Z"]
				},
				"input": {
					"type": "array",
					"minItems": 1,
					"items": {
						"type": "object",
						"properties": {
					  		"address_ref": {
								"type": "string",
								"pattern": "^cryptocurrency-wallet--",
								"description": "Defines the STIX ID of a cryptocurrency-wallet object",
								"examples": ["cryptocurrency-wallet--b6ae6083-875f-5fe9-84b9-89e168f89406"]
					  		},
					  		"amount": {
								"type": "number",
								"description": "The input amount.",
								"examples": [0.84000000]
							},
          					"minItems": 1
						}
					}
				},
				"output": {
				 	"type": "array",
				 	"minItems": 1,
				 	"items": {
						"type": "object",
						"properties": {
					  		"address_ref": {
								"type": "string",
								"pattern": "^cryptocurrency-wallet--",
								"description": "Defines the STIX ID of a cryptocurrency-wallet object",
								"examples": ["cryptocurrency-wallet--b6ae6083-875f-5fe9-84b9-89e168f89406"]
					  		},
					  		"amount": {
								"type": "number",
								"description": "The output amount.",
								"examples": [0.04000000]
					  		}
						}
					}
				},
				"extensions": {
					"type": "object",
					"properties": {
						"extension-definition--151d042d-4dcf-5e44-843f-1024440318e5": {
							"type": "object",
							"properties": {
								"extension_type": {
									"enum": ["new-sco"]
								}
							},
							"required": ["extension_type"]
						}
					},
					"required": ["extension-definition--151d042d-4dcf-5e44-843f-1024440318e5"]
				}
			}
		}
	],
	"required": [
		"type",
		"id",
		"extensions"
	]
}